{
	"name": "jamomatic",
	"version": "0.3.0",
	"description": "A library dedicated to working with the Korean alphabet, Hangul.",
	"main": "dist/index.cjs",
	"type": "module",
	"exports": {
		"import": "./dist/index.mjs",
		"require": "./dist/index.cjs"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jacoblockett/jamomatic.git"
	},
	"scripts": {
		"build": "rollup -c",
		"test": "node --test tests/*.js",
		"dev": "clear && nodemon --quiet dev/index.js"
	},
	"keywords": [
		"hangul",
		"korean",
		"strings",
		"utility",
		"utilities",
		"jamo"
	],
	"files": [
		"dist/",
		"LICENSE",
		"README.md"
	],
	"license": "MIT",
	"author": "Jacob Lockett",
	"devDependencies": {
		"@rollup/plugin-commonjs": "^29.0.0",
		"@rollup/plugin-node-resolve": "^16.0.3",
		"benchmark": "^2.1.4",
		"rollup": "^4.54.0"
	}
}
